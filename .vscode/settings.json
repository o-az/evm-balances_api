{
  "solidity.compileUsingRemoteVersion": "0.8.9+commit.e5eed63a",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    "extensions": [
      ".js",
      ".json",
      ".md",
      ".mdx",
      ".ts",
      ".tsx"
